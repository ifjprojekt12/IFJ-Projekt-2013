<?php

//prevod retezcu na desetine cisla
$str="0.1232e3";
$double = strval($str);
$put=put_string($double, "\n");

$str=0.0;
$double = strval($str);
$put=put_string($double, "\n");

$stdsa = 0;
$duble = strval($stdsa);
$ut = put_string($duble, "\n");

$stdsa = 3.32132;
$duble = strval($stdsa);
$ut = put_string($duble, "\n");

$stdsa = null;
$duble = strval($stdsa);
$ut = put_string($duble, "\n");

$stdsa = true;
$duble = strval($stdsa);
$ut = put_string($duble, "\n");

$stdsa = false;
$duble = strval($stdsa);
$ut = put_string($duble,":D", "\n");

$stdsa = "false";
$duble = strval($stdsa);
$ut = put_string($duble, "\n");
